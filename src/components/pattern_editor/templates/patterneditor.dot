<div class="pattern-editor">
  <div style="float: left;">
    <div>
      <table id="timeline-header">
        <thead>
          <tr class="row">
            <th class="tick"><div class="timeline-header"><br /></div></th>
          </tr>
        </thead>
      </table>
    </div>
    <div id="timeline">
     {{#def.timeline}}
    </div>
  </div>
  <div class="xscroll">
    <div id="trackheader" class="leftSideTable" style="width: 1070px;">
     {{#def.header}}
    </div>
    <div class="sideTable">
      <table id="trackview">
        {{#def.trackview}}
      </table>
    </div>
  </div>
</div>
