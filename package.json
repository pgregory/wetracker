{
  "name": "wetracker",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --inline",
    "build": "webpack --progress --hide-modules --config webpack.config.js",
    "build:dev": "NODE_ENV=dev npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "postinstall": "NODE_ENV=production npm run build",
    "start": "node server.js",
    "lint:staged": "lint-staged",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern server.js --ignore-pattern webpack.config.js",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:fix": "npm run lint:eslint -- --fix ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "file-loader": "^4.2.0",
    "font-awesome-loader": "^1.0.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^8.0.0",
    "sharedworker-loader": "^2.1.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "url-loader": "^2.2.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "arraybuffer-loader": "^1.0.8",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.0",
    "director": "^1.2.8",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.4",
    "gridstack": "^4.2.6",
    "immutable": "^3.8.2",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "jquery.fancytree": "^2.33.0",
    "json-loader": "^0.5.7",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lz4-asm": "^0.3.3",
    "marko": "^4.18.24",
    "marko-loader": "^1.5.0",
    "mousetrap": "^1.6.3",
    "query-string": "^6.8.3",
    "raw-loader": "^3.1.0",
    "regenerator-runtime": "^0.13.3",
    "style-loader": "^1.0.0",
    "tab64": "0.0.1",
    "text-encoding": "^0.7.0",
    "tunajs": "^1.0.11",
    "url": "^0.11.0",
    "webpack": "^4.41.2",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true,
      "jest": true
    },
    "plugins": [],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "no-restricted-globals": [
        "error",
        "event",
        "fdescribe"
      ],
      "no-multi-spaces": [
        2,
        {
          "ignoreEOLComments": true
        }
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 0,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "require-yield": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./webpack.config.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|wepb|svg|ttf|woff|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
