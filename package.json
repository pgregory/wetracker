{
  "name": "wetracker",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --inline",
    "build": "webpack --progress --hide-modules --config webpack.config.js",
    "build:dev": "NODE_ENV=dev npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "postinstall": "NODE_ENV=production npm run build",
    "start": "node server.js",
    "lint:staged": "lint-staged",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern server.js --ignore-pattern webpack.config.js",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "stylelint ./src/**/*.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^19.0.2",
    "lint-staged": "^3.3.1",
    "pre-commit": "^1.2.2",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack-dev-server": "^1.16.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "arraybuffer-loader": "^0.2.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-plugin-transform-exponentiation-operator": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "director": "^1.2.8",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.4",
    "gridstack": "^0.2.6",
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "json-loader": "^0.5.4",
    "jstree": "^3.3.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lz4-asm": "^0.3.3",
    "marko": "^3.13.0",
    "marko-loader": "^1.0.0",
    "mousetrap": "^1.6.0",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "shared-worker-loader": "^0.1.0",
    "style-loader": "^0.13.1",
    "tab64": "0.0.1",
    "text-encoding": "^0.6.4",
    "tunajs": "^1.0.0",
    "url": "^0.11.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "worker-loader": "^0.7.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true,
      "jest": true
    },
    "plugins": [],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 0,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "require-yield": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./webpack.config.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "jest": {
    "moduleFileExtensions": ["js"],
    "moduleDirectories": ["node_modules"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|wepb|svg|ttf|woff|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
